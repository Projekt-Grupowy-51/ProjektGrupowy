{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/{controller}/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5001
        }
      ],
      "UpstreamPathTemplate": "/api/general-backend/{controller}/{everything}",
      "UpstreamHttpMethod": [
        "Get",
        "Post",
        "Put",
        "Delete",
        "Options"
      ],
      "SwaggerKey": "app"
    },
    {
      "DownstreamPathTemplate": "/hub/app/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5002
        }
      ],
      "UpstreamPathTemplate": "/notifications/hub/app/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post" ],
      "UseWebSockets": true
    }
  ]
}