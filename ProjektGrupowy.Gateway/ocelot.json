{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/{controller}/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "app",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/general-backend/{controller}/{everything}",
      "UpstreamHttpMethod": ["Get", "Post", "Put", "Delete", "Options"],
      "SwaggerKey": "app"
    },
    {
      "DownstreamPathTemplate": "/hub/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [{ "Host": "notifications", "Port": 80 }],
      "UpstreamPathTemplate": "/api/notifications/hub/{everything}",
      "UpstreamHttpMethod": ["Get", "Post", "Options"],
      "UseWebSockets": true,
      "SwaggerKey": "notifications"
    }
  ]
}
